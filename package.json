{
  "name": "gdrive-uploader",
  "version": "2.0.0",
  "description": "A comprehensive Google Drive file upload and management library for Node.js, NestJS, React, and Next.js applications",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./nestjs": {
    "types": "./dist/nestjs/index.d.ts",
    "default": "./dist/nestjs/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "FRONTEND_ARCHITECTURE.md"
  ],
  "keywords": [
    "google-drive",
    "google-drive-api",
    "file-upload",
    "nestjs",
    "react",
    "nextjs",
    "typescript",
    "drive",
    "storage",
    "cloud-storage",
    "oauth2",
    "pkce",
    "react-hooks",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Thakurkartik30/gdrive-asset-uploader-.git"
  },
  "bugs": {
    "url": "https://github.com/Thakurkartik30/gdrive-asset-uploader-/issues"
  },
  "homepage": "https://github.com/Thakurkartik30/gdrive-asset-uploader-#readme",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "googleapis": "^152.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@types/express": "^5.0.6",
    "@types/node": "^22.13.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
